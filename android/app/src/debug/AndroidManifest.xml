<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <!-- The INTERNET permission is required for development. Specifically, the Flutter tool needs it to communicate with the running application to allow setting breakpoints, to provide hot reload, etc. -->
    <uses-permission android:name="android.permission.INTERNET"/>

    <!-- Firebase Messaging Service with tools:replace to resolve the conflict -->
    <service android:name="com.google.firebase.messaging.FirebaseMessagingService"
        android:exported="true"
        tools:replace="android:exported">
        <!-- Other service configurations -->
    </service>

</manifest>
